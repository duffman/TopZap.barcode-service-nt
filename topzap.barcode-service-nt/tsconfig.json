{
  "compilerOptions": {
    "types": ["reflect-metadata", "node"],
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ESNEXT",
    "noImplicitAny": false,
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "@channels/*": ["./app/channels.git/*"],
      "@webapp/*": ["./app/webapp/*"],
      "@app/*": ["./app/*"],
      "@api-clients/*": ["./miners/*"],
      "@cli/*": ["./app/cli/*"],
      "@lib/*": ["./lib/*"],
      "@putte/*": ["./lib/putte-ts/*"],
      "@inc/*": ["./inc/*"],
      "@utils/*": ["./lib/utils/*"]
    }
  },
  "exclude": [
    "dist",
    "crap",
    "node_modules"
  ]
}
