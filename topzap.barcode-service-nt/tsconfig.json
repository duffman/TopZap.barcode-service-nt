{
  "compilerOptions": {
    "lib": ["es6", "dom" ],
    "types": ["node", "reflect-metadata", "inversify"],
    "module": "commonjs",
    "target": "ESNEXT",
    "noImplicitAny": false,
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@pubsub/*": ["./lib/topzap.pubsub-lib.git/*"],
      "@webapp/*": ["./app/webapp/*"],
      "@root/*": ["./*"],
      "@app/*": ["./app/*"],
      "@api/*": ["./app/api/*"],
      "@components/*": ["./app/components/*"],
      "@scaledrone/*": ["./app/pubsub/scaledrone-service/*"],
      "@api-clients/*": ["./api-clients/*"],



      "@core/*": ["./app/core/*"],
      "@cli/*": ["./app/cli/*"],
      "@lib/*": ["./lib/*"],
      "@inc/*": ["./inc/*"],
      "@db/*": ["./app/database/*"],

      "@zapModels/*": ["./app/zap-ts-models/*"],
      "@models/*": ["./app/models/*"],
      "@putteDb/*": ["./lib/dbcore/*"],
      "@utils/*": ["./lib/utils/*"],
      "@putte/*": ["./lib/putte-ts/*"],
      "@zaplib/*": ["./lib/topzap-utils-lib/*"]
    }
  },
  "exclude": [
    "test",
    "node_modules"
  ]
}